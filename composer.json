{
  "name": "tpay-com/tpay-openapi-php",
  "type": "library",
  "license": "LGPL-3.0",
  "description": "Tpay OpenAPI library",
  "extra": [
    {
      "engine": "PHP SDK"
    }
  ],
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Tpay",
      "homepage": "https://tpay.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "ext-curl": "*",
    "ext-json": "*",
    "phpseclib/phpseclib": "^2|^3"
  },
  "require-dev": {
    "php-parallel-lint/php-parallel-lint": "^1.3.2"
  },
  "support": {
    "issues": "https://github.com/tpay-com/tpay-openapi-php/issues",
    "email": "support@tpay.com",
    "docs": "https://openapi.tpay.com"
  },
  "autoload": {
    "psr-4": {
      "tpaySDK\\": ""
    },
    "exclude-from-classmap": ["**/Examples/"]
  }
}
